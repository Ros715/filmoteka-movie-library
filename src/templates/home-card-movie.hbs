{{#each this}}
<li class="card-library list" data-date="{{release_date}}" data-title="{{title}}" data-img="{{poster_path}}" data-vote="{{vote_average}}" data-votes="{{vote_count}}" data-popularity="{{popularity}}" data-original="{{original_title}}" data-genres="{{genre_ids}}" data-description="{{overview}}">
    <img class="img-card-library" src="https://image.tmdb.org/t/p/original{{poster_path}}" alt=""/>
    <div class="movie-information">
        <h3 class="movie-name">{{title}}</h3>
        <div class="generes-rating">
            <p class="genres">{{genre_ids}} | {{release_date}}</p>  
        </div>
    </div>
</li>
{{/each}}
